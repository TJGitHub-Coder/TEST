<script>
function getCookie(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(';');
  for(var i = 0; i <ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}
var val = ''
if (getCookie("loggin") == 0) {
  val = 'Login';
  loc = 'login';
} else {
  val = 'Logout';
  loc = 'logout';
};
document.getElementById("loginout").innerHTML = val;
document.getElementById("loginout").href = loc + '.html';
</script>
